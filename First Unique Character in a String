class Solution {
public:
    int firstUniqChar(string s) {
        int uniq[26] = {0};
        for(int i=0; i<s.size(); i++){
            uniq[s[i]-97]++;
        }
        for(int i=0; i<s.size(); i++){
            if(uniq[s[i]-97] == 1){
                return i;
            }
        }
        return -1;
    }
};
