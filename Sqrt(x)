class Solution {
public:
    int mySqrt(int x) {
        float mid,l=0,r=x;
        while(l<=r){            //INCOMPLETE CODE
            mid=(l+r)/2;
            int t=mid*mid;
            if(t==x) return mid;
            if(t>x) r=mid;
            else l=mid;
        }
        return -1;
    }
};
